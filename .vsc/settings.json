{
    "window.zoomLevel": 2,
    "editor.fontSize": 14,
    "editor.hover.enabled": true,
    "workbench.startupEditor": "none",
    "explorer.compactFolders": false,
    "terminal.integrated.fontSize": 14,
    "editor.rulers": [80, 120],
    "code-runner.executorMap": {
      "python": "clear ; python -u"
    },
    "code-runner.runInTerminal": true,
    "code-runner.ignoreSelection": true,
    "editor.fontFamily": "Consolas, 'Dank Mono', 'Source Code Pro', 'Fira Code', Menlo, 'Inconsolata', 'Droid Sans Mono', 'DejaVu Sans Mono', 'Ubuntu Mono', 'Courier New', Courier, Monaco, monospace",
    "terminal.integrated.fontFamily": "Fira Code",
    "[python]": {
      "editor.tabSize": 4,
      "editor.insertSpaces": true,
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "ms-python.autopep8",
      
    
  
      "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.fixAll.unusedImports": "explicit",
        "source.organizeImports": "explicit"
      }
    },
    "python.languageServer": "Pylance",
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.analysis.diagnosticSeverityOverrides": {},
    "python.analysis.typeCheckingMode": "basic",
    "cSpell.enabled": false,
    "workbench.colorCustomizations": {
      "activityBar.activeBackground": "#791387",
      "activityBar.background": "#791387",
      "activityBar.foreground": "#e7e7e7",
      "activityBar.inactiveForeground": "#e7e7e799",
      "activityBarBadge.background": "#422c74",
      "activityBarBadge.foreground": "#e7e7e7",
      "commandCenter.border": "#e7e7e799",
      "sash.hoverBorder": "#791387",
      "statusBar.background": "#791387",
      "statusBar.foreground": "#e7e7e7",
      "statusBarItem.hoverBackground": "#791387",
      "statusBarItem.remoteBackground": "#791387",
      "statusBarItem.remoteForeground": "#e7e7e7",
      "titleBar.activeBackground": "#791387",
      "titleBar.activeForeground": "#e7e7e7",
      "titleBar.inactiveBackground": "#791387",
      "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.color": "#215732"
    
  }
  